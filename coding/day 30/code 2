/*Imagine you are building a stock price prediction tool ðŸ“ˆ.

The array represents stock prices recorded at different times.

You want to help users find the next price jump â€” that is, for each time, 
tell the user the next time the stock price rises.

If the stock price never rises again, show -1.

Sample Testcase:1
------------------------
input=5
2 1 2 4 3
Output=4 2 4 -1 -1



*/
import java.util.*;

public class StockPricePrediction {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        
        int n = sc.nextInt(); // Number of stock prices
        int[] prices = new int[n];
        for (int i = 0; i < n; i++) {
            prices[i] = sc.nextInt();
        }
        
        int[] ans = new int[n];
        Stack<Integer> stack = new Stack<>();
        
        for (int i = n - 1; i >= 0; i--) {
            // Pop all smaller or equal elements
            while (!stack.isEmpty() && stack.peek() <= prices[i]) {
                stack.pop();
            }
            
            // If stack is empty, no next greater element
            ans[i] = stack.isEmpty() ? -1 : stack.peek();
            
            // Push current price into stack
            stack.push(prices[i]);
        }
        
        for (int val : ans) {
            System.out.print(val + " ");
        }
    }
}


import java.util.*;

public class Solution{
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        
        int n = sc.nextInt();
        int[] prices = new int[n];
        
        for (int i = 0; i < n; i++) {
            prices[i] = sc.nextInt();
        }
        
        int[] ans = new int[n];
        
        for (int i = 0; i < n; i++) {
            int nextGreater = -1;
            for (int j = i + 1; j < n; j++) {


                if (prices[j] > prices[i]) {


                    nextGreater = prices[j];
                    break;
                }
        }
            ans[i] = nextGreater;
        }
        
        for (int val : ans) {
            System.out.print(val + " ");
        }
    }

}
